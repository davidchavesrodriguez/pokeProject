{% extends 'base.html.twig' %}

{% block title %}Modify Pokemon{% endblock %}

{% block content %}
    <h2>Modify Pokemon</h2>
    {{ form_start(form) }}

    {{ form_row(form.name) }}
    {{ form_row(form.type) }}
    {{ form_row(form.abilities) }}
    {{ form_row(form.moves) }}
    {{ form_row(form.sprite) }}

    <button type="submit">Save</button>

    {{ form_end(form) }}
{% endblock %}
